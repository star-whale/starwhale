{
  "author": "lwz",
  "license": "Apache-2.0",
  "name": "@starwhale/core",
  "version": "1.0.0",
  "description": "Starwhale Runtime Library",
  "keywords": [
    "starwhale",
    "typescript"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "http://github.com/star-whale/starwhale.git",
    "directory": "packages/starwhale-runtime"
  },
  "main": "src/index.ts",
  "types": "src/index.ts",
  "publishConfig": {
    "main": "build/index.js",
    "module": "build/esm/index.js",
    "types": "build/index.d.ts",
    "access": "public"
  },
  "files": [
    "build",
    "./README.md",
    "./CHANGELOG.md",
    "LICENSE_APACHE2"
  ],
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json && rollup -c rollup.config.ts",
    "bundle": "rollup -c rollup.config.ts",
    "clean": "rimraf ./build ./compiled ./package.tgz",
    "typecheck": "tsc --emitDeclarationOnly false --noEmit",
    "start": "vite --host 0.0.0.0"
  },
  "dependencies": {
    "@aksel/structjs": "^1.0.0",
    "@sentry/browser": "6.19.7",
    "base64-js": "^1.5.1",
    "eventemitter3": "^4.0.7",
    "history": "4.10.1",
    "lodash": "4.17.21",
    "rxjs": "^7.5.7",
    "tsc": "^2.0.4",
    "tslib": "2.4.0"
  },
  "devDependencies": {
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@testing-library/dom": "8.13.0",
    "@testing-library/react": "12.1.4",
    "@testing-library/user-event": "14.4.3",
    "@types/angular": "1.8.4",
    "@types/history": "4.7.11",
    "@types/jest": "28.1.6",
    "@types/lodash": "4.14.182",
    "@types/react": "17.0.42",
    "@types/react-dom": "17.0.14",
    "@types/systemjs": "^0.20.6",
    "@vitejs/plugin-react": "^2.2.0",
    "esbuild": "0.15.7",
    "lodash": "4.17.21",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "3.0.2",
    "rollup": "2.79.1",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-esbuild": "4.10.1",
    "rollup-plugin-node-externals": "^4.1.0",
    "rollup-plugin-sourcemaps": "0.6.3",
    "rollup-plugin-terser": "7.0.2",
    "typescript": "^4.9.3",
    "vite": "^3.2.2",
    "vite-plugin-dts": "^1.7.1"
  },
  "peerDependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  }
}