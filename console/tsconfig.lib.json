    "declaration": true,
    "declarationMap": true,
    "noEmit": false,
    "emitDeclarationOnly": true,
    "isolatedModules": true,
    "noFallthroughCasesInSwitch": true,
    "composite":true,
  },
  "composite":true,
  "exclude": ["dist/**/*"],
  "extends": "./tsconfig.base.json",
  "include": [
    "src/**/*.ts*",
    "./@types/*.d.ts",
  ]
