.POHNY: download
download:
	mkdir -p data/fgvc-aircraft
	[ -f data/fgvc-aircraft-2013b.tar.gz ] || wget https://www.robots.ox.ac.uk/~vgg/data/fgvc-aircraft/archives/fgvc-aircraft-2013b.tar.gz -O data/fgvc-aircraft-2013b.tar.gz
	[ -d data/fgvc-aircraft-2013b ] || tar -xzvf data/fgvc-aircraft-2013b.tar.gz -C data
