{
  "name": "starwhale-docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "serve": "docusaurus serve",
    "clear": "docusaurus clear",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "ci": "yarn lint && yarn prettier:diff",
    "lint": "eslint --cache \"**/*.js\" && stylelint \"**/*.css\"",
    "prettier": "prettier --config .prettierrc --write \"**/*.{js,md}\"",
    "prettier:diff": "prettier --config .prettierrc --list-different \"**/*.{js,md}\"",
    "lint-docs": "markdownlint docs i18n/zh  && markdownlint --disable=MD041 ../README.md"
  },
  "dependencies": {
    "@antv/x6": "^1.32.2",
    "@cmfcmf/docusaurus-search-local": "^0.6.2",
    "@docusaurus/core": "2.1.0",
    "@docusaurus/plugin-content-blog": "2.1.0",
    "@docusaurus/plugin-google-analytics": "2.1.0",
    "@docusaurus/plugin-google-gtag": "2.1.0",
    "@docusaurus/preset-classic": "2.1.0",
    "@easyops-cn/docusaurus-search-local": "^0.16.3",
    "@mdx-js/react": "^1.6.21",
    "clsx": "^1.1.1",
    "docusaurus-plugin-sass": "^0.2.2",
    "markdownlint": "^0.25.1",
    "markdownlint-cli": "^0.31.1",
    "medium-zoom": "^1.0.6",
    "prism-react-renderer": "^1.2.0",
    "raw-loader": "^4.0.2",
    "rc-form": "^2.4.12",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-hook-form": "^7.31.2",
    "sass": "^1.52.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "prettier": "^2.0.2",
    "stylelint": "^13.2.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
